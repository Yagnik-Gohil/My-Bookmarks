{
    "name": "My Bookmarks",
    "version": "0.0.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "default.html",
        "default_icon": "logo.png"
    },
    "icons": {
        "128": "logo.png"
    },
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "js": [
                "jquery.js",
                "background.js"
            ],
            "matches": [
                "http://*/*",
                "https://*/*"
            ]
        }
    ]
}